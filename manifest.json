{
   "browser_action": {
      "default_icon": {
         "128": "icons/128x128.png",
         "19": "icons/19x19.png",
         "38": "icons/38x38.png",
         "48": "icons/48x48.png"
      },
      "default_popup": "popup.html",
      "default_title": "Splice Sounds Shortcuts"
   },
   "content_scripts": [ {
      "js": [ "js/jquery.min.js", "js/content.js" ],
      "matches": [ "*://splice.com/sounds*", "*://*.splice.com/sounds*" ]
   } ],
   "icons": {
      "128": "icons/128x128.png",
      "19": "icons/19x19.png",
      "38": "icons/38x38.png",
      "48": "icons/48x48.png"
   },
   "description": "Adds helpful keyboard shortcuts for navigating through the Splice Sounds web interface",
   "manifest_version": 2,
   "name": "Splice Sounds Shortcuts",
   "version": "0.1.6"
}
